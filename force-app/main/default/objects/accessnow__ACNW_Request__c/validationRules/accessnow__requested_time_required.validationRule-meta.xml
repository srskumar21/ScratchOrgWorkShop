<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>accessnow__requested_time_required</fullName>
    <active>true</active>
    <errorConditionFormula>isblank(accessnow__Requested_Start_Time__c) &amp;&amp; !accessnow__Provision_Immediately__c</errorConditionFormula>
