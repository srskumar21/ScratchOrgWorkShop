<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Amount_Greater_than_Zero</fullName>
    <active>true</active>
    <errorConditionFormula>AND( $Profile.Name  = &quot;GSS Sales - ANZ/UK&quot;, UnitPrice = 0)</errorConditionFormula>
    <errorMessage>The amount of a product cannot be equal to 0</errorMessage>
</ValidationRule>
